generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  memID     String   @id
  password  String
  name      String?
  email     String
  createdAt DateTime @default(now())
}

model Tag {
  id        String   @id @default(uuid())
  name      String
  createdAt DateTime @default(now())
}